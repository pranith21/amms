<project name="iq-util jar builder" default="start">
	<target name="start">
		<echo>${makeJarMsg}</echo>
		<jar destfile="${build.home}/jar/${jar.name}.jar" basedir="${output.dir}">
			<include name="**" />
			<exclude name="iq/test/**" />
			<manifest>
				<attribute name="Implementation-Title" value="${jar.name}.jar" />
				<attribute name="Implementation-Vendor" value="iquesters" />
				<attribute name="Implementation-Version" value="${build.version}" />
				<attribute name="Built-Date" value="${build.date}" />
				<attribute name="Built-Time" value="${build.time}" />
				<attribute name="Built-By" value="${build.user}" />
			</manifest>
		</jar>
	</target>
</project>