<project name="iq-switch-client Initiation" default="start">
	<target name="start">
		<echo>${initializeMsg}</echo>
		<antcall target="createDir"></antcall>
	</target>
	
	<target name="clean">
		<delete dir="${build.home}" />
	</target>

	<target name="createDir" depends="clean">
		<mkdir dir="${build.home}/jar" />
		<mkdir dir="${build.home}/src" />
	</target>
</project>